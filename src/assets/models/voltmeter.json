    "min": 0,
    "step": 10
  }, {
    "id": "stop",
    "label": "stop time",
    "level": 2,
    "unit": "ms",
    "value": 10000,
    "input": "valueSlider",
    "max": 10000,
    "min": 0,
    "step": 10
  }]
